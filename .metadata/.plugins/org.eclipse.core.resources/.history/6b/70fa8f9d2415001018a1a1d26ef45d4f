package oops;

public class MyClass {
	int a,b;
	MyClass(){
		a=0;
		b=0;
	}
	
	MyClass(int i){
		a = i;
		b = 0;
	}
	
	MyClass(int i, int j){
		a = i;
		b = j;
	}
	
	public static void main(String[] args) {
		MyClass obj1 = new MyClass();
		MyClass obj2 = new MyClass(10);
		MyClass obj3 = new MyClass(10,20);
		
	}
}