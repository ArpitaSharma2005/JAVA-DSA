package oops;
abstract class Bank
{
	abstract int rateOfInterest();
}

class Savings extends Bank
{
	int rateOfInterest()
	{
		return 3;
	}
}

class Recurring	 extends Bank
{
	int rateOfInterest()
	{
		return 6;
	}
}

class Fixed extends Bank
{
	int rateOfInterest()
	{
		return 9;
	}
}

public class Abstractions {
	public static void main(String[] args) {
		Bank bank;
		bank = new Savings();
		System.out.println("Rate of interest for savings account is : "+bank.rateOfInterest());
		bank = new Recurring();
		System.out.println("Rate of interest for recurring account is: "+bank.rateOfInterest());
		bank = new Fixed();
		System.out.println("Rate of interest for fixed account is: "+bank.rateOfInterest());
		
	}
}
