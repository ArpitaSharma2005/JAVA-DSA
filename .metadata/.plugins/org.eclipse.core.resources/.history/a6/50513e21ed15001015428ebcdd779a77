package com.examples;

import com.example.classes.*;
import com.example.intf.*;

public class Application {
	public static void main(String[] args) {

		Car swift = new Swift();
		Corola corola = new Corola();  // Type change kiya, taaki setEngine call kar sake

		Engine engine = new Engine();
		engine.setType("v6");  // Type ko manually set kiya

		corola.setEngine(engine);  // Corola ko engine diya

		System.out.println(swift.getCar());       // Output: Maruti Swift
		System.out.println(corola.getCar());      // Output: Toyota from Corola with engine type as: v6
		System.out.println(engine.getType());     // Output: v6
	}
}
